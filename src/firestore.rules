rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    // Default-deny all reads and writes for security.
    match /{document=**} {
      allow read, write: if false;
    }

    // This rule allows any authenticated user to read and write to the
    // sandboxed data collections used by the application.
    match /artifacts/{appId}/public/data/{collection}/{document=**} {
        allow read, write: if request.auth != null;
    }
  }
}
