
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows read and write access to all users, including those
    // who are not signed in. This is for debugging purposes and should be
    // replaced with more secure rules before production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
